{
  "manifest_version": 3,
  "name": "CpURL",
  "version": "0.0.0",
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Shift+Ctrl+U",
        "mac": "Shift+Command+U"
      }
    }
  },
  "description": "Copy tabs URL as several styles by shortcuts",
  "permissions": ["tabs"]
}